      printf("                             @@@@@           #*--==+@                 \n");                       
      printf("                                              +++**@                  \n");
      printf("\n");
      printf("Tipo: Terra\nhp: 42\nAtaque: 21\nDefesa: 24\nVelocidade: 19");
      printf("Habilidade 1: Tiro de lama (40 power)\nHabilidade 2:Enfrentar (40 power)");
      break;
    case 14:
      printf("Deseja adicionar Diglett à sua equipe?(s/n)\n");
      printf("\n");
      printf("                                *###########                          \n");           
      printf("                              ****+===+++****##@@@                    \n");             
      printf("                            **++========++++****####                  \n");             
      printf("                            **+-:====--==+++++***#@@                  \n");             
      printf("                           ***+*#+===##==+++++****###                 \n");             
      printf("                           ##+=+*====**+++++++****#@@                 \n");             
      printf("                           ##+*******+++++++++*****###                \n");             
      printf("                           ###+---=++##*++++++******#@                \n");             
      printf("                           ##*********##*+++++******#@                \n");             
      printf("                           ##*##@@@@@@##+++++*******#@                \n");             
      printf("                           ##++********++++++*******###               \n");             
      printf("                          ###***+++++++++************#@@              \n");             
      printf("              ###@@@    ##*##************************#@@@             \n");             
      printf("             ##*++*#@  #**###************************#@##@@@@         \n");             
      printf("             ##***##@*****#**######*************####*#@###+*#@@@@     \n");             
      printf("               @@@@ *+---=###--=++##**####**##@@#*##@#++++=*##+=+#@   \n");             
      printf("                   +++--=*+=:::-===*@*===*#@*+==+*****==+*#+----+#@   \n");             
      printf("                   :---=++=----===++**+==+#@+---===****+**#######@@   \n");              
      printf("                    ======================+++======+++*++*=+@@@@@     \n");             
      printf("                            ===============================           \n");             
      printf("                                 =====      =====                     \n");

      printf("\n");
      printf("Tipo: Terra\nhp: 30\nAtaque: 20\nDefesa: 17\nVelocidade: 35");
      printf("Habilidade 1: Tiro de lama (40 power)\nHabilidade 2:Enfrentar (40 power)");
      break;
    case 15:
      printf("Deseja adicionar Cubone à sua equipe?(s/n)\n");
      printf("\n");
      printf("                            #+++#   ######    +++                  \n");                 
      printf("                           #=.:=#++-:.::-*#++:.+#                  \n");                
      printf("                            #*-=-:.....:-=-..:+##                  \n");                
      printf("                             *+.......:::::::=##                   \n");                
      printf("                ####        #+.......:+###:::-*#                   \n");                
      printf("          #####+:.:=#      #+-......=*###*+=--=*#                  \n");                
      printf("         ##=..+*-:-*#      #+.....:-##*+-*#-*=-+**#                \n");                
      printf("         #+....---+#       #=....::#*##*:*#-*=---+#                \n");                
      printf("          ##*+*--:+#     +=:....:::+###*++-=+=+*##                 \n");                
      printf("               #-..-#   #:...:-==:::::-+##*--=##                   \n");                
      printf("                #=..-#  #=::-+###+-:==:-+=#--##                    \n");                
      printf("                 #+.:=#  #=:-=++*#=-*#####*#####                   \n");                
      printf("                  #+::*####*+*##**##***********#####               \n");                
      printf("                 #*=+#-#+====+*********+====#**+===+#              \n");                
      printf("                 ##***#==*+***###***=--------*###*+++##            \n");                
      printf("                   ###===### ##****=----------*# ##=###            \n");               
      printf("                            ##*===#=----------*##                  \n");                
      printf("                         ####+=--==**=--------*#*#                 \n");                
      printf("                    #####+##+=======#+------=+***##                \n");                
      printf("                    ##====##+=======#*======#****##                \n");                
      printf("                     #######**++++**####**##****###                \n");                
      printf("                           ##**+***##    ##*###+=+#                \n");                
      printf("                            #*:.:##                                \n");

      printf("\n");
      printf("Tipo: Terra\nhp: 42\nAtaque: 22\nDefesa: 31\nVelocidade: 17");
      printf("Habilidade 1: Tiro de lama (40 power)\nHabilidade 2:Enfrentar (40 power)");
      break;
    default:
      printf("O número precisa ser de 1 a 15\n");
  }
